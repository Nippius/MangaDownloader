<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!-- How many pages to download concurrently. -->
    <add key="maxConcurrentPageDownloads" value="10" />
    <!-- Number of milliseconds to wait before downloading next chapter. -->
    <add key="delayBetweenChapters" value="2000" />
    <!-- Base URL to download images from. -->
    <add key="urlBase" value="http://img.mangastream.to" />
    <!-- "folder" containing the images-->
    <add key="urlImageFolder" value="/manga" />
    <!-- Wich manga to download. -->
    <add key="urlMangaName" value="/naruto" />
    <!--Image file extension -->
    <add key="urlExtension" value=".png" />
    <!-- Wich chapter to start downloading from. -->
    <add key="startChapter" value="661" />
    <!-- Last chapter to download. -->
    <add key="endChapter" value="661" />
    <!-- Folder where the manga will be downloaded to. -->
    <add key="SavePath" value="c:\Naruto\" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
</configuration>